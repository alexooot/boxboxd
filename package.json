{
  "name": "boxboxd",
  "private": true,
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:generate": "cd packages/database && npm run db:generate",
    "db:studio": "cd packages/database && npm run db:studio"
  },
  "devDependencies": {
    "turbo": "latest"
  }
}
